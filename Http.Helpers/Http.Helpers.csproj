<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net5.0</TargetFrameworks>
    <Company>ZTn</Company>
    <Authors>ZTn</Authors>
    <Product>ZTn C# Helpers</Product>
    <PackageId>ZTn.Http.Helpers</PackageId>
    <RootNamespace>ZTn.Http.Helpers</RootNamespace>
    <Copyright>ZTn 2020</Copyright>
    <Description>A very simple library providing some extension methods around System.Net.Http.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyName>ZTn.Http.Helpers</AssemblyName>
    <PackageProjectUrl>https://github.com/zetoken/Http.Helpers</PackageProjectUrl>
    <PackageTags>csharp netstandard http</PackageTags>
    <Version>1.0.0</Version>
    <PackageLicenseUrl></PackageLicenseUrl>
    <RepositoryUrl>https://github.com/zetoken/Http.Helpers</RepositoryUrl>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageLicenseFile></PackageLicenseFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
</Project>